@page
@model Login_Register.Pages.LogoutModel
@{
}

<form method="post">
    <dir class="container mt-5">

        <div class="row justify-content-center">
            <div class="col-sm-12 col-md-12 col-lg-4">
                <p>Are you sure you want to Logout?</p>
                <p>
                    <button class="btn btn-primary" asp-page-handler="Logout">Yes</button>
                    <button class="btn btn-light m-lg-5 border-dark" asp-page-handler="DontLogout">No</button>
                </p>
            </div>
        </div>

    </dir>
</form>